
table.table.table-striped
  thead.table-dark.black.white-text
    tr
      th(scope='col')  #
      th(scope='col')  Application Name
      th(scope='col')  Database Name
      th(scope='col')  Service Name
      th(scope='col')  Service Type
      th(scope='col')  Database Service Details
      th(scope='col')  Database Package
      th(scope='col')  Service Health
  tbody
    each dat, i in db1DataList
      tr
        th(scope='row') #{i+1}
        td  #{dat.application_name}
        td  #{dat.appl_database_name}
        td  #{dat.appl_db_service_name}
        td  #{dat.appl_db_service_type}
        td  #{dat.appl_db_service_details}
        td  #{dat.appl_db_package_name}
        if dat.appl_db_service_operation_health === 1
          td.table-success
        else
          td.table-danger